{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true
  },
  "baseUrl": ".",
  "paths": {
    "@components/*": [
      "./src/components/*"
    ],
    "@styles/*": [
      "./src/assets/styles/*"
    ],
    "@images/*": [
      "./src/assets/images/*"
    ],
    "@config/*": [
      "./src/config/*"
    ],
    "@type/*": [
      "./src/type/*"
    ],
    "@interfaces/*": [
      "./src/interface/*"
    ]
  }
}
