{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "typeRoots": ["./node_modules/@types", "@types"],
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"],
      "@screen/*": ["./src/screen/*"],
      "@type/*": ["./src/types/*"],
      "@lib/*": ["./src/libs/*"],
      "@asset/*": ["./src/assets/*"]
    }
  }
}
